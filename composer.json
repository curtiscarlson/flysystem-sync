{
    "name": "thadbryson/flysystem-sync",
    "description": "Flysystem plugin to sync files and directories.",
    "minimum-stability": "stable",
    "license": "MIT",
    "authors": [
        {
            "name": "Thad Bryson",
            "email": "thadbry@gmail.com",
            "homepage": "http://thadbryson.co"
        }
    ],
    "require": {
        "php": ">= 7.2",
        "league/flysystem": "1.*",
        "illuminate/console": "5.8.*",
        "illuminate/events": "5.8.*",
        "illuminate/filesystem": "5.8.*"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "codeception/codeception": "3.*"
    },
    "autoload": {
        "psr-4": {
            "TCB\\Flysystem\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Unit\\": "tests/unit/"
        }
    }
}
